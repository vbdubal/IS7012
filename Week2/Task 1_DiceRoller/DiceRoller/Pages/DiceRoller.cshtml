@page
@model DiceRoller.Pages.DiceRollerModel
@{ int randomNumber = new Random().Next(1, 7); //GENERATES NUMBER BETWEEN 1 and 6
   int winningNumber = 3; }



<h1>Random number generated is: @randomNumber</h1>
@if (randomNumber == winningNumber)
{
<h3>That's a WIN!! :) Jackpot! </h3>
<img src="data:image/jpeg;base64," class="img-rounded" alt="YAY"> }



else
{
<h3>Oh Oh, try again!! </h3>
@if (randomNumber - winningNumber < 0)
    {
     <h3>That's @(winningNumber - randomNumber) far off. </h3>
     <img src="https://i.pinimg.com/originals/83/de/27/83de270fb8b652185f3486e853cb907d.jpg" class="img-rounded" alt="NAY"> }
else
    {
    <h3>That's @(randomNumber - winningNumber) far off. </h3>
    <img src="https://i.pinimg.com/originals/83/de/27/83de270fb8b652185f3486e853cb907d.jpg" class="img-rounded" alt="NAY">}}
